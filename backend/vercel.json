{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/projects(.*)",
      "dest": "/api/projects$1"
    },
    {
      "src": "/api/tasks(.*)",
      "dest": "/api/tasks$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}